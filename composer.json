{
    "name": "hexlet/code",
    "description": "hexlet project",
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Php\\Package\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "justbepanda",
            "email": "justbepanda@gmail.com"
        }
    ],
    "require": {
        "tightenco/collect": "^9.52",
        "phpstan/phpstan": "^1.10",
        "docopt/docopt": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.8",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/extension-installer": "^1.3",
        "symfony/var-dumper": "^6.4",
        "psy/psysh": "@stable"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
